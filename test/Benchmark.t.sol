        vm.deal(account, 10 ether);

        vm.startPrank(account);
        SafeTransferLib.safeApprove(token0, _UNISWAP_V2_ROUTER_ADDRESS, 1 ether);
        SafeTransferLib.safeApprove(token1, _UNISWAP_V2_ROUTER_ADDRESS, 1 ether);
