            address(paymentToken), _PIMLICO_PAYMASTER_V06, type(uint128).max
        );
        SafeTransferLib.safeApprove(
            address(paymentToken), _PIMLICO_PAYMASTER_V07, type(uint128).max
        );
        vm.stopPrank();
    }

